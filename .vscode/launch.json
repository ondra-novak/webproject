    "version": "0.2.0",
    "configurations": [{
        "name": "(gdb) Spustit",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/bin/webproject",
        "args": ["-R/home/ondra/Obrázky","-o","${workspaceFolder}/web/out/index.html","-mh","-s","localhost:10000","${workspaceFolder}/web/src/main.js"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Povolit přehledný výpis pro gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Nastavte variantu zpětného překladu na Intel.",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
        ]
    }]
}